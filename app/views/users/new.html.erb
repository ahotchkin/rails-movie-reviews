<h1>Sign Up</h1>

<%= form_for @user, url: "/signup" do |f| %>
  <%= f.label "Username:" %>
  <%= f.text_field :username %>
  <br><br>
  <%= f.label "Email:" %>
  <%= f.email_field :email %>
  <br><br>
  <%= f.label "Password:" %>
  <%= f.password_field :password %>
  <br><br>
  <%= f.submit "Sign Up" %>
<% end %>
